  "scripts": {
    "typecheck": "flow check",
    "lint": "eslint -c .eslintrc packages/**",
    "test": "mocha --timeout 10000 --parallel --jobs 2 --require .mocha-setup.js packages/**/*.spec.js",
    "test:divergent": "mocha --timeout 10000 --require .mocha-setup.js packages/**/*.spec.js",
    "test:integration": "jest",
    "test:integration:all-coverage": "jest --coverage",
    "test:integration:branch-coverage": "jest --coverage --changedSince=main",
    "pretty": "prettier --write '**/*.{js,scss,md}' '!packages/profiler/public' '!**/CHANGELOG.md'",
    "bootstrap": "lerna bootstrap --use-workspaces",
    "start": "f() { REACT_APP_TARGET=$1 REACT_APP_USER=$2 REACT_APP_PASS=$3 REACT_APP_MFA=$4 lerna exec --scope @kitman/profiler -- yarn start; }; f",
    "start-legacy": "f() { REACT_APP_TARGET=$1 lerna exec --scope @kitman/profiler -- yarn start-legacy; }; f",
    "start:mock": "REACT_APP_MOCK_API=true yarn start",
    "build": "lerna exec --scope @kitman/profiler -- yarn build",
    "build-legacy": "lerna exec --scope @kitman/profiler -- yarn build-legacy",
    "ci:detect-flakey-tests": "node scripts/detectFlakeyTests.mjs $JIRA_USERNAME $JIRA_API_TOKEN $CIRCLECI_API_TOKEN",
    "ci:publish": "lerna publish --force-publish @kitman/profiler --loglevel=verbose",
    "ci:publish-canary": "lerna publish --force-publish --canary --preid beta --tag-version-prefix beta --loglevel=verbose",
    "ci:publish-patch": "lerna publish patch",
    "postpublish": "node scripts/postpublish.js",
    "preversion": "node scripts/preversion.js",
    "storybook": "yarn storybook:components",
    "storybook:components": "lerna exec --scope @kitman/components -- start-storybook -p 8008",
    "storybook:playbook": "lerna exec --scope @kitman/playbook -- yarn storybook",
    "build-storybook": "lerna exec --scope @kitman/components -- build-storybook",
    "prepare": "husky install",
    "prepublishOnly": "yarn build-legacy",
    "replace-ff": "node ./scripts/replace-feature-flag.mjs 'packages/**/*.js'"
  },
  "msw": {
    "workerDirectory": "packages/profiler/public"
  },
